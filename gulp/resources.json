{
  "toMove": [
    "app/assets/favicon*",
    "!app/assets/**/*.html",
    "app/**/*.html",
    "app/assets/fonts/*"
  ],
  "injectDev": {
    "site": [
      "bower_components/normalize-css/normalize.css",
      "app/assets/css/style.css",

      "bower_components/jquery/dist/jquery.js",

      "bower_components/jquery.easing/js/jquery.easing.js",
      "bower_components/PACE/pace.js",
      "bower_components/bLazy/blazy.js",
      "bower_components/imagesloaded/imagesloaded.pkgd.js",
      "bower_components/waypoints/lib/jquery.waypoints.js",
      "bower_components/animejs/anime.js",

      "app/assets/js/app.js",
      "app/assets/js/chj-detect.js",
      "app/assets/js/chj-device.js",
      "app/assets/js/chj-anime.js",
      "app/assets/js/chj-scrollto.js",
      "app/assets/js/chj-blazy.js",
      "app/assets/js/chj-toggle.js",
      "app/assets/js/chj-waypoints.js"
    ]
  },
  "injectDist": {
    "site": [
      "dist/assets/css/style-min.css",
      "dist/assets/js/site-min.js"
    ]
  }
}
